{
  "name": "cardano-js",
  "version": "0.4.8",
  "description": "Cardano primitives for ECMAScript applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "shx rm -rf dist && tsc",
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly  src/index.ts",
    "dev:test": "tsc --watch --noEmit && yarn test --watchAll",
    "lint": "eslint \"src/**/*.ts\"",
    "prepare": "yarn lint && yarn build && yarn test",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/input-output-hk/cardano-js.git"
  },
  "keywords": [
    "Cardano",
    "Blockchain"
  ],
  "contributors": [
    "Rhys Bartels-Waller <rhys.bartelswaller@iohk.io> (https://iohk.io)",
    "Sam Jeston <sam.jeston@iohk.io> (https://iohk.io)",
    "Matthias Benkort <matthias.benkort@iohk.io> (https://iohk.io)"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/input-output-hk/cardano-js/issues"
  },
  "homepage": "https://github.com/input-output-hk/cardano-js#readme",
  "devDependencies": {
    "@types/bech32": "^1.1.1",
    "@types/bs58": "^4.0.0",
    "@types/cbor": "^2.0.0",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.12.32",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^24.9.0",
    "prettier": "^2.0.4",
    "shx": "^0.3.2",
    "ts-jest": "^24.0.2",
    "typescript": "3.5.3"
  },
  "dependencies": {
    "bech32": "^1.1.3",
    "bignumber.js": "^9.0.0",
    "bip39": "^3.0.2",
    "blakejs": "^1.1.0",
    "bs58": "^4.0.1",
    "cbor": "^4.3.0",
    "js-chain-libs-node": "^0.3.0",
    "ts-custom-error": "^3.1.1"
  },
  "files": [
    "dist/*",
    "!dist/__test__",
    "!dist/**/*.spec.*",
    "lib",
    "LICENSE",
    "LICENSE_NOTICE"
  ],
  "engines": {
    "node": ">=10.0.0"
  }
}
